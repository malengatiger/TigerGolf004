<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:tools="http://schemas.android.com/tools"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:baselineAligned="false"
              android:divider="?android:attr/dividerHorizontal"
              android:orientation="horizontal"
              android:showDividers="middle"
              tools:context=".WorkStationListActivity">

    <fragment
            android:id="@+id/GL_fragmentMaster"
            android:name="com.boha.malengagolf.admin.leaderboard.fragments.TournamentPanelFragment"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2"
            tools:layout="@android:layout/list_content"/>

    <fragment
            android:id="@+id/GL_fragmentDetail"
            android:name="com.boha.malengagolf.admin.leaderboard.fragments.ContainerFragment"
            android:layout_height="match_parent"
            android:layout_width="0dp"
            android:layout_weight="3"
            tools:layout="@android:layout/list_content"/>
    <fragment
            android:id="@+id/GL_fragmentSplash"
            android:name="com.boha.malengagolf.library.fragments.LeaderBoardSplashFragment"
            android:layout_height="match_parent"
            android:layout_width="0dp"
            android:layout_weight="0"
            tools:layout="@android:layout/list_content"/>


</LinearLayout>